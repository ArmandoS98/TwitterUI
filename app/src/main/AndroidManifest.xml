<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techun.twitterui">

    <application
        android:name=".Twitter"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TwitterUI">
        <activity
            android:name=".ui.drawerNavigationView.ui.HelpCenterActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.SettingsAndPrivacyActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.TwitterForProfessionalsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.MonetizationActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.MomentsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.BookmarksActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.TopicsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.ListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.drawerNavigationView.ui.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>